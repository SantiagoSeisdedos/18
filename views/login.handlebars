<h1>Login</h1>
<form id="loginForm">
  <label for="email">Email</label>
  <input type="email" id="email" name="email" placeholder="Email" /><br />
  <label for="password">Password</label>
  <input
    type="password"
    id="password"
    name="password"
    placeholder="Password"
  /><br />
  <button type="submit">Log in</button>
</form>
<script src="js/login.js"></script>