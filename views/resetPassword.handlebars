<section>
  <h1>Reestablecer contraseña</h1>
  <form id="resetPasswordForm">
    <div class="form-group">
      <label for="inputEmail">Email</label>
      <input
        type="email"
        id="inputEmail"
        name="email"
        placeholder="email"
        autocomplete="on"
      />
    </div>

    <button type="submit">Reestablecer contraseña</button>
  </form>
  <p><a href="/login">Iniciar sesion</a></p>
</section>

<script src="js/profile/resetPassword.js"></script>